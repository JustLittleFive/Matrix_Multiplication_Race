{
  "tasks": [
    {
      "type": "cppbuild",
      "label": "C/C++: gcc-11 生成活动文件",
      "command": "/usr/bin/gcc-11",
      "args": [
        "-fdiagnostics-color=always",
        "-g",
        "${fileDirname}/*.c",
        "-o",
        "${fileDirname}/${fileBasenameNoExtension}",
        "-O3",
        "-I /usr/include/x86_64-linux-gnu/",
        "-L/usr/lib/x86_64-linux-gnu/",
        "-lopenblas"
      ],
      "options": {
        "cwd": "${fileDirname}"
      },
      "problemMatcher": [
        "$gcc"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "detail": "调试器生成的任务。"
    }
  ],
  "version": "2.0.0"
}